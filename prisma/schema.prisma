generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Currency {
  id            Int     @id @default(autoincrement())
  index         Int     @unique
  name          String
  image         String
  fullname      String
  buy           Decimal
  sell          Decimal
  price_history Json[]
}
